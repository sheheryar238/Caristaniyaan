
@{
    ViewBag.Title = "Carappointment";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="banner">
    <div class="box h-200 bg-image-13">
        <div class="box-overlay text-left">
            <div class="container">
                <h1 class="box-heading box-heading-md p-b-0">Car Appointment</h1>
                <ul class="breadcrumbs">
                    <li><a href="home.aspx">Home</a></li>
                    <li> Car Appointment</li>
                </ul>
            </div>
        </div>
    </div>
</section>
<section class="bg-pri-c relative bg-f5">
    <div class="container">
        <div class="row">
            <div class="col-sm-6 col-sm-offset-3">
                <div class="box-1 p-50">
                    <!-- Nav tabs --><!-- Tab panes -->
                    <h4 class="line-default f-30 selected">Enter Details For Appointment</h4>
                    <form>
                        <input type="text" class="form-item" placeholder="Model Year">
                        <input type="text" class="form-item" placeholder="Car Info">
                        <input type="text" class="form-item" placeholder="Your name">
                        <input type="text" class="form-item" placeholder="Appointment Date & Time ( dd:mm:yy & hh:mm)">
                        <input type="text" class="form-item" placeholder="Phone">
                        <input type="text" class="form-item" placeholder="Email">
                        <textarea class="form-item h-200 p-15" placeholder="Messenge"></textarea>
                        <div class="checkbox">
                            <label><input type="checkbox" value="">I agree to the terms of service.</label>
                        </div>
                        <button type="submit" class="btn bg-dc4c46">Send</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>

